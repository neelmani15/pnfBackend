{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
      "routes": [
          {
            "src": "/(.*)",
            "dest": "/"
          }
        ],
        "crons": [
          {
            "path": "/",
            "schedule": "10 16 * * *"
          }
        ]

  }